<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/keyword"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    android:background="@drawable/some_id"
    >

    <TextView
        android:id="@+id/text_keyword"
        android:layout_width="160dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="23dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:text="@string/some_id25"
        android:textAppearance="@style/some_id7"
        android:lineSpacingExtra="-4sp"
        android:gravity="top"
        android:textStyle="bold"
        />


    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/some_id"
        android:layout_below="@+id/text_keyword"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/keyword_first"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#음료, 음식"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/text_beverage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="맛있는 음료"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/keyword_first"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_beverage"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_beverage"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="3.0" />

            <TextView
                android:id="@+id/text_dessert"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="맛있는 디저트"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@+id/rangeSlider_beverage"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_dessert"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_dessert"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="3.0" />


            <TextView
                android:id="@+id/text_various"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="다양한 메뉴"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/rangeSlider_dessert"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_various"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_various"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="3.0" />


            <TextView
                android:id="@+id/text_special"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="특별한 메뉴"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/rangeSlider_various"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_special"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_special"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="3.0" />

            <TextView
                android:id="@+id/keyword_second"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#시설"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_below="@id/rangeSlider_special"/>

            <CheckBox
                android:id="@+id/check_parking"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/keyword_second"
                android:text="주차"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                />

            <TextView
                android:id="@+id/text_size"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="카페 크기"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/check_parking"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_size"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_size"
                android:valueFrom="1.0"
                android:valueTo="3.0"
                android:stepSize="1.0"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                />

            <TextView
                android:id="@+id/text_Landscape"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="풍경"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/rangeSlider_size"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_Landscape"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_Landscape"
                android:valueFrom="1.0"
                android:valueTo="3.0"
                android:stepSize="1.0"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                />


            <TextView
                android:id="@+id/keyword_third"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#분위기"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_below="@id/rangeSlider_Landscape"
                />

            <TextView
                android:id="@+id/text_talk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="대화하기 좋음"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@+id/keyword_third"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                />

            <TextView
                android:id="@+id/text_concentration"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="집중하기 좋음"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@+id/keyword_third"
                android:layout_alignParentRight="true"
                android:layout_marginRight="25dp"
                android:layout_marginTop="5dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_concentration"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_concentration"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="4.0" />


            <TextView
                android:id="@+id/text_trendy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="트렌디함"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_below="@id/rangeSlider_concentration"
                android:layout_marginLeft="15dp"
                />

            <com.google.android.material.slider.RangeSlider
                android:id="@+id/rangeSlider_trendy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_trendy"
                android:layout_centerInParent="true"
                android:layout_marginStart="20dp"
                android:stepSize="1.0"
                android:valueFrom="1.0"
                android:valueTo="3.0" />

            <TextView
                android:id="@+id/keyword_fourth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#가격대"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_below="@+id/rangeSlider_trendy"/>

            <!--            <TextView-->
            <!--                android:id="@+id/text_price"-->
            <!--                android:layout_width="wrap_content"-->
            <!--                android:layout_height="wrap_content"-->
            <!--                android:text="Price"-->
            <!--                android:textSize="14dp"-->
            <!--                android:textStyle="bold"-->
            <!--                android:layout_below="@+id/keyword_fourth"-->
            <!--                android:layout_marginLeft="15dp"-->
            <!--                android:layout_marginTop="5dp"-->
            <!--                />-->

            <!--            <com.google.android.material.slider.RangeSlider-->
            <!--                android:id="@+id/rangeSlider_price"-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="wrap_content"-->
            <!--                android:layout_below="@+id/text_price"-->
            <!--                android:valueFrom="1.0"-->
            <!--                android:valueTo="3.0"-->
            <!--                android:stepSize="1.0"-->
            <!--                android:layout_centerInParent="true"-->
            <!--                android:layout_marginStart="20dp"-->
            <!--                />-->

            <CheckBox
                android:id="@+id/check_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/keyword_fourth"
                android:text="가성비"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                />

            <TextView
                android:id="@+id/keyword_fifth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="#시설"
                android:textSize="17dp"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_below="@id/check_price"/>

            <CheckBox
                android:id="@+id/check_gift"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/keyword_fifth"
                android:text="선물 포장"
                android:textSize="13dp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                />

        </RelativeLayout>

    </ScrollView>

    <TextView
        android:id="@+id/some_id"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_above="@+id/btn_next_keyword"
        android:layout_centerInParent="true"
        android:layout_marginBottom="8dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="1sp"
        android:text="@string/some_id27"
        android:textAppearance="@style/some_id14" />

    <TextView
        android:id="@+id/btn_next_keyword"
        android:layout_width="360dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/rectangle_15"
        android:text="완료"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:textAlignment="center"
        android:gravity="center"
        />


</RelativeLayout>